<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  <property name="PI" value="3.1415926535897931" />
  <link name="base_link" >
    <visual>
      <geometry>
	<box size="0.455 .204 .239"/>
      </geometry>
      <origin xyz="-0.1125 0 .1185"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<box size="0.455 .204 .239"/>
      </geometry>
      <origin xyz="-0.1125 0 .1185"/>
    </collision>
  </link>

  <joint name="j1" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-169*PI/180}" upper="${169*PI/180}" velocity="0.5"/>
    <parent link="base_link"/>
    <child link="shoulder_block"/>
    <origin xyz="0 0 0.239"/>
  </joint>


  <link name="shoulder_block">
    <visual>
      <geometry>
	<box size="0.200 .327 .111"/>
      </geometry>
      <origin xyz="-.100 0 0.0555"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<box size="0.200 .327 .111"/>
      </geometry>
      <origin xyz="-.100 0 0.0555"/>
    </collision>
  </link>

  <joint name="j2" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-92*PI/180}" upper="${131*PI/180}" velocity="0.5"/>
    <parent link="shoulder_block"/>
    <child link="shoulder"/>
    <origin xyz="0.085 0 0.111" rpy="1.57079632679 0 3.14159265359"/>
  </joint>


  <link name="shoulder">
    <visual>
      <geometry>
	<cylinder length="0.327" radius="0.085"/>
      </geometry>
      <origin xyz="0 0 0"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.327" radius="0.085"/>
      </geometry>
      <origin xyz="0 0 0"/>
    </collision>
  </link>


  <joint name="fixed_joint_1" type="fixed">
    <axis xyz="0 0 1"/>
    <parent link="shoulder"/>
    <child link="upper_arm"/>
    <origin xyz="0 0.085 0"/>
  </joint>

  <link name="upper_arm">
    <visual>
      <geometry>
	<box size="0.090 0.210 0.02"/>
      </geometry>
      <origin xyz="0 0.105 -0.102"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<box size="0.090 0.110 0.02"/>
      </geometry>
      <origin xyz="0 0.19 -0.102"/>
    </collision>
  </link>

  <joint name="j3" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-128*PI/180}" upper="${165*PI/180}" velocity="0.5"/>
    <parent link="upper_arm"/>
    <child link="elbow"/>
    <origin xyz="0 0.295 0" rpy="3.14159265359 0 1.57"/>
  </joint>

  <link name="elbow">
    <visual>
      <geometry>
	<cylinder length="0.327" radius="0.085"/>
      </geometry>
      <origin xyz="0 0 0"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.327" radius="0.085"/>
      </geometry>
      <origin xyz="0 0 0"/>
    </collision>
  </link>


  <joint name="fixed_joint_2" type="fixed">
    <parent link="elbow"/>
    <child link="elbow_block"/>
    <origin xyz="0 0.100 0"/>
  </joint>

  <link name="elbow_block">
    <visual>
      <geometry>
	<box size="0.225 .126 .239"/>
      </geometry>
      <origin xyz="-0.0275 0 0"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<box size="0.225 .126 .239"/>
      </geometry>
      <origin xyz="-0.0275 0 0"/>
    </collision>
  </link>

  <joint name="j4" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-159*PI/180}" upper="${159*PI/180}" velocity="0.5"/>
    <parent link="elbow_block"/>
    <child link="fore_arm"/>
    <origin xyz="0.085 0 0"  rpy="0 1.5707963267 0"/>
  </joint>

  <link name="fore_arm">
    <visual>
      <geometry>
	<box size="0.170 .120 .230"/>
      </geometry>
      <origin xyz="0 0 0.115"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<box size="0.225 .126 .239"/>
      </geometry>
      <origin xyz="-0.0275 0 0"/>
    </collision>
  </link>

  <joint name="j5" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-110*PI/180}" upper="${110*PI/180}" velocity="0.5"/>
    <parent link="fore_arm"/>
    <child link="wrist"/>
    <origin xyz="0 0 0.240"  rpy="3.14159265359 -1.5707963267 0"/>
  </joint>

  <link name="wrist">
    <visual>
     <geometry>
	<box size="0.170 .09 .080"/>
      </geometry>
      <origin xyz="0 0 0"/> 
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
     <geometry>
	<box size="0.170 .09 .080"/>
      </geometry>
      <origin xyz="0 0 0"/> 
    </collision>
  </link>

  <joint name="j6" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="${-359*PI/180}" upper="${359*PI/180}" velocity="0.5"/>
    <parent link="wrist"/>
    <child link="end_effector"/>
    <origin xyz="0.085 0 0"  rpy="0 1.5707963267 0"/>
  </joint>

  <link name="end_effector">
  </link>

</robot>
